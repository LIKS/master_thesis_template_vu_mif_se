% Citing bachelor's thesis. We use thesis entry type and specify type={bathesis}
@thesis{PvzBakLt,
    author = {A. Pavardonis},
    title = {Bakalauro darbo pavadinimas},
    school = {Universiteto pavadinimas},
    type = {bathesis},
    address = {Vilnius},
    year = {2005},
    langid = {lithuanian},
}

